PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>

SELECT DISTINCT * WHERE 
 { # Substitute in your own project URI from projects.dfid.gov.uk here...   
  ?r4dProject dcterms:identifier <http://projects.dfid.gov.uk/project.aspx?Project=<%= prid %>>.    
  ?r4dProject dcterms:title ?projectTitle.      
  ?output dcterms:isPartOf ?r4dProject.   
  ?output dcterms:title ?outputTitle.  
  ?output dcterms:date ?outputDate.

  OPTIONAL {?output dcterms:creator ?outputCreatorID.
  ?outputCreatorID foaf:name ?outputCreator. }  
  OPTIONAL {?output dcterms:description ?outputDescription. }
  

} LIMIT 200